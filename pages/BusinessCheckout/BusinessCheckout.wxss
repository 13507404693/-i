/* pages/BusinessCheckout/BusinessCheckout.wxss */
page {
    background: #F0F0F0;
}

.checkout-contriner {
    background: url(data:image/png;base64,) no-repeat center top/100% 370rpx;
    padding-bottom: 118rpx;
}

.header {
    padding: 0 30rpx;
    margin-bottom: 38rpx;
}

.header .title {
    font-size: 46rpx;
    color: #FFF;
    padding-top: 18rpx;
}

.header .address {
    color: #FFF;
    font-size: 32rpx;
    padding-top: 40rpx;
}

.cell-pd {
    padding: 48rpx 0;
}

.cell-mg {
    margin: 0 30rpx;
}

.mg-top20 {
    margin-top: 20rpx !important;
}

.wrap-cell {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}

.wrap-cell:nth-last-child(1) {
    border-bottom: none;
}

.cell-ft {
    text-align: right;
}

.cell-bd {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

.cell-line-b {
    border-bottom: 1px solid  #f0f0f0;
}

.bd-font {
    font-weight: 400;
    font-size: 32rpx;
    color: #333;
}

.ft-font {
    font-size: 32rpx;
    color: #333;
}

.cell-access {
    width: 24rpx;
    height: 24rpx;
    margin-left: 12rpx;
}

.wraper {
    margin: 0 30rpx;
    background: #FFF;
}

.cell-checked {
    padding: 36rpx 0;
}

.cell-checked .checkbox {
    font-size: 24rpx;
    color: #666;
}

.cell-checked .checked {
    transform: scale(.7);
    vertical-align: middle;
}

.cell-checked .checkbox .text-a {
    color: #525980;
}

.business-title {
    padding: 28rpx 0;
    font-size: 30rpx;
    color: #333;
}

.goods-img {
    width: 168rpx;
    height: 104rpx;
    border-radius: 4rpx;
}

.goods-item {
    margin: 30rpx 0;
    width: 100%;
    -webkit-align-items:flex-start;
    align-items:flex-start;
}

.goods-info {
    padding-left: 20rpx;
}

.goods-name {
    font-size: 28rpx;
    color: #333;
    padding-bottom: 20rpx;
}

.goods-num {
    font-size: 24rpx;
    color: #999;
    padding-left: 48rpx;
    padding-right: 24rpx;
}

.price-box .price {
    font-size: 28rpx;
    font-weight: bold;
    color: #333;
}

.price-box .price .unit {
    font-size: 22rpx;
    font-weight: 500;
}

.price-box .org-price {
    font-size: 24rpx;
    font-weight: bold;
    color: #999;
    margin-top: 20rpx;
    text-decoration: line-through;
}

.price-box .org-price .unit {
    font-size: 18rpx;
    font-weight: 500;
}

.pay-info .info-item {
    width: 100%;
    margin: 32rpx 0;
    font-size: 30rpx;
    color: #333;
}

.pay-info .info-item .integral {
    font-size: 28rpx;
}

.goods-item .trait{
    font-size:20rpx;
    color: #999;
}

.cell-sum {
    padding: 38rpx 0;
    font-size: 28rpx;
    color: #323233;
}

.cell-sum .sum {
    font-size: 40rpx;
    font-weight: bold;
    padding-left: 20rpx;
}

.cell-sum .sum .unit {
    font-size: 28rpx;
    font-weight: 500;
}

.cell-remarks {
    font-size: 30rpx;
    padding: 30rpx 0;
    color: #333;
}

.cell-remarks .cell-ft {
    color: #999;
}

.footer-bar {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 10;
    width: 100%;
    height: 98rpx;
    line-height: 98rpx;
    background: #4D4B4B;
}

.footer-bar .cell-bd {
    width: 100%;
    color: #fff;
    padding-left: 30rpx;
    padding-right: 42rpx;
}

.footer-bar .discount {
    font-size: 26rpx;
    padding-top: 6rpx;
    box-sizing: border-box;
    max-width: 186rpx;
}

.footer-bar .discount .sum {
    font-size: 30rpx;
}

.footer-bar .discount .unit {
    font-size: 24rpx;
}

.footer-bar .total-price {
    font-size: 32rpx;
}

.footer-bar .total-price .sum {
    font-size: 40rpx;
    font-weight: bold;
}

.footer-bar .total-price .unit {
    font-size: 32rpx;
    font-weight: 400;
    padding-left: 10rpx;
}

.submit-order {
    width: 240rpx;
    height: 98rpx;
    line-height: 98rpx;
    padding: 0;
    margin: 0;
    border-radius: 0;
    background: linear-gradient(180deg, rgba(254, 109, 66, 1) 0%, rgba(229, 46, 49, 1) 100%);
    font-size: 34rpx;
    color: #FFF;
}
.submit-order[disabled]:not([type]){
    background: rgba(128,128,128,1);
    color: #fff;
}
.orderMsg{
    font-size:30rpx;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 0;
    height: 0;
    background: #000;
    opacity: 0;
    z-index: 100;
    transition: opacity .3s;
}

.mask.active {
    width: 100%;
    height: 100%;
    opacity: .6;
}

.popup-mask.active {
    width: 100%;
    height: 100%;
    opacity: 1;
}

.paytype-popup {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    background: #fff;
    box-sizing: border-box;
    opacity: 0;
    transform: translateY(100%);
    -webkit-transform: translateY(100%);
    -webkit-transition: 0.2s ease;
    transition: 0.2s ease;
}
.paytype-popup.active {
    opacity: 1;
    transform: translateY(0);
    -webkit-transform: translateY(0);
    -webkit-transition: 0.3s ease;
    transition: 0.3s ease;
}
.paytype-popup .popup-header{
    padding: 40rpx 0;
    text-align: center;
    border-bottom: 2rpx solid #F0F0F0;
}
.paytype-popup .popup-header .title{
    font-size:32rpx;
    color: #000;
}
.paytype-popup .icon-close{
    position: absolute;
    top: 40rpx;
    right: 30rpx;
    width: 30rpx;
    height: 30rpx;
}
.popup-wraper{
    height: 590rpx;
}
.popup-wraper .type-item{
    text-align: center;
    font-size:32rpx;
    font-weight:400;
    padding: 40rpx 0;
}
.popup-wraper .type-item.active{
    color: #1C71F1;
}